/users/{userId}:
  get:
    description: |
        ...
        (mapping):
            outputType: application/vnd.people.query.user+json
            type: query
            name: people.get-user1
        (mapping):
            outputType: application/vnd.people.query.user2+json
            type: query
            name: people.get-user2
